{
  "devDependencies": {
    "@battis/eslint-config": "^1.1.4",
    "@battis/prettier-config": "^1.3.2",
    "npm-run-all": "^4.1.5"
  },
  "prettier": "@battis/prettier-config",
  "scripts": {
    "setup": "run-s setup:*",
    "setup:env": "run-s env",
    "setup:terraform": ". .env; terraform -chdir=terraform/ -var=\"google_project_id=$GOOGLE_CLOUD_PROJECT\" --var=\"google_region=$GOOGLE_REGION\"",
    "setup:deploy": "run-s deploy",
    "deploy": ". .env; gcloud app deploy --project=$GOOGLE_CLOUD_PROJECT --quiet"
  }
}
